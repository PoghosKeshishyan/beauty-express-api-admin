<h2><%= title %></h2>

<form method="post" action="/feedback_headings/edit/<%= feedback_heading.id %>">
    <label>Feedback heading:</label><br>
    <input type="text" name="heading" value="<%= feedback_heading.heading %>"><br>

    <button type="submit">Update</button>
</form>

<% if (error) { %>
    <p style="color:red;"><%= error %></p>
<% } %>