<h2><%= title %></h2>

<form method="post" action="/navbars/edit/<%= navbar.id %>">
    <label>Title:</label><br>
    <input type="text" name="title" value="<%= navbar.title %>"><br>

    <label>Url:</label><br>
    <input type="text" name="url" value="<%= navbar.url %>"><br>
    
    <button type="submit">Update</button>
</form>

<% if (error) { %>
    <p style="color:red;"><%= error %></p>
<% } %>