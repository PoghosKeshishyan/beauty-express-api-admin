<h2><%= title %></h2>

<form method="post" action="/logos/create">
    <label>Image:</label><br>
    <input type="text" name="image"><br>

    <label>Title:</label><br>
    <input type="text" name="title"><br>
    
    <button type="submit">Create</button>
</form>

<% if (error) { %>
    <p style="color:red;"><%= error %></p>
<% } %>